{
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:react/recommended"
    ],
    "settings": {
        "react": {
            "version": "16.8"
        }
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "no-console": "off",
        "import/namespace": "off"
    }
}
